<div class="row">
  <div class="span4">

    <%= simple_form_for(resource, :as => resource_name, :url => registration_path(resource_name), html:{class: "well"}) do |f| %>
      <%= devise_error_messages! %>
      
      <form>
        <fieldset>
          <legend>Sign up</legend>
          <div><%= f.input :first_name %><br />
        
          <div><%= f.input :last_name %><br />
        
          <div><%= f.input :profile_name %><br />
        
          <div><%= f.input :email %><br />
          <!-- <%= f.email_field :email, :autofocus => true %></div> -->
        
          <div><%= f.input :password %><br />
        
          <div><%= f.input :password_confirmation %><br />
        
          <div><%= f.button :submit, "Sign up" %></div>
        <fieldset>
      <form>
    <% end %>
    <%= render "devise/shared/links" %>
  </div>
</div>        
    

